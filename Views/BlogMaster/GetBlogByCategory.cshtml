</head>
<body>

    <div class="container">
        <input type="hidden" id="hfCatID" value=@ViewBag.CatID />
        <br />
        <div class="row">
            <img src="~/sss.PNG" width="100%" height="30%" />
        </div>
    </div>
    <br />
    @*<div>
            <label id="lbPacketDetails">

            </label>
        </div>*@

